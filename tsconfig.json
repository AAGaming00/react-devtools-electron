{
	"compilerOptions": {
		"strict": true,
		"noImplicitAny": true,
		"declaration": true,
		"incremental": true,
		"tsBuildInfoFile": "./.cache/library.tsbuildinfo",
		"outDir": "dist",
		"target": "es5",
		"removeComments": true,
		"forceConsistentCasingInFileNames": true,
		"lib": [
			"es2015",
			"dom"
		]
	},
	"include": [
		"src"
	]
}
